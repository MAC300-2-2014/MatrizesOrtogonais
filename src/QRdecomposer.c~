/*******************************
 * Retorna valor máximo de vetor
 * tipo double.
 *******************************/
double maximo(double x, int n) {
  int i;
  double m = x[0];

  for (i = 1; i < n; i++) {
    if (x[i] > m) 
      m = x[i];
  }

  return m;
}

